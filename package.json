{
  "name": "@tremho/MistLift",
  "version": "0.1.0-pre-release.2",
  "description": "lift command line utility for MistLift development",
  "main": "build/index.js",
  "bin": {
    "lift": "build/lift.js"
  },
  "scripts": {
    "prepublish": "rimraf build && npm run build && npm run indev",
    "clean": "rimraf build && rimraf node_modules && npm install",
    "build": "run-script-os",
    "build:windows": "tsc && npm run postbuild",
    "build:nix": "tsc && chmod +x build/lift.js && npm run postbuild",
    "test": "tap --reporter=base --color --passes --disable-coverage src/lib/Tests/**/*.test.ts",
    "indev": "run-script-os",
    "indev:nix": "echo nix",
    "indev:windows": "echo windows",
    "postbuild": "cp -r src/commands/builtin/prebuilt-zips build/commands/builtin"
  },
  "files": [
    "build/**/*",
    "src/**/*",
    "index.d.ts",
    "README.md",
    "tsconfig.json"
  ],
  "keywords": [
    "MistLift",
    "Cloud",
    "Lift",
    "development",
    "framework",
    "cli",
    "aws",
    "express",
    "node"
  ],
  "author": "tremho",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-api-gateway": "^3.490.0",
    "@aws-sdk/client-lambda": "^3.490.0",
    "@tremho/inverse-y": "^1.0.1-pre-release.259",
    "ansi-colors": "^4.1.3",
    "axios": "^1.6.5",
    "body-parser": "^1.20.2",
    "clear-module": "^4.1.2",
    "express": "^4.18.2",
    "openapi-ui": "^1.0.4",
    "openapi3-ts": "^4.2.1",
    "readline-sync": "^1.4.10",
    "zip-dir": "^2.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^16.4.12",
    "@types/tap": "^15.0.11",
    "@types/ws": "^8.2.0",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6",
    "tap": "^18.6.1",
    "typescript": "^5.3.3"
  }
}
